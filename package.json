{
  "name": "hackflix",
  "version": "1.0.0",
  "author": "Victor <victor@hackages.io>",
  "description": "A Vue.js project",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write 'src/**/*.ts'",
    "format:vue": "prettier --write 'src/**/*.vue'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "4.0.0",
    "lodash": "^4.17.5",
    "vee-validate": "^2.0.4",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vue-strap": "^1.1.40"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.1",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.1",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^1.10.2",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "git add"
    ]
  }
}
